{
  "version": 2,
  "functions": {
    "simple-frontend-server.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/marketplace-inspired.html"
    },
    {
      "source": "/admin-futuristic.html",
      "destination": "/admin-futuristic.html"
    },
    {
      "source": "/test-purchase.html",
      "destination": "/test-purchase.html"
    },
    {
      "source": "/marketplace-inspired.html", 
      "destination": "/marketplace-inspired.html"
    },
    {
      "source": "/(.*)",
      "destination": "/simple-frontend-server.js"
    }
  ]
}